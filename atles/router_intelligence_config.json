{
  "router_test": {
    "test_count": 5,
    "test_delay": 0.5,
    "enable_comprehensive_testing": true,
    "test_all_task_types": true
  },
  "logging": {
    "request_max_length": 30,
    "include_confidence_scores": true,
    "include_reasoning": true,
    "log_level": "INFO"
  },
  "ui": {
    "router_test_button": {
      "text": "ðŸ§  Test Router Intelligence",
      "bg_color": "#4CAF50",
      "fg_color": "white",
      "font": ["Arial", 10, "bold"],
      "tooltip": "Test the Router Intelligence system with dynamic requests"
    },
    "router_status_display": {
      "show_model_usage": true,
      "show_confidence_scores": true,
      "show_reasoning": false,
      "update_interval": 1.0
    }
  },
  "request_generation": {
    "enable_dynamic_requests": true,
    "max_requests_per_cycle": 3,
    "request_generation_interval": 5.0,
    "context_aware_generation": true,
    "history_based_selection": true
  },
  "performance": {
    "enable_monitoring": true,
    "track_decision_accuracy": true,
    "track_model_performance": true,
    "generate_performance_reports": true,
    "report_interval": 300
  },
  "safety": {
    "max_concurrent_requests": 10,
    "request_timeout": 30.0,
    "enable_error_recovery": true,
    "fallback_to_simulation": true,
    "log_all_errors": true
  },
  "router_usage": {
    "scope": "medium",
    "intensity": "medium",
    "scope_levels": {
      "minimal": {"main_loop": 0.2, "goal_gen": 0.1, "goal_comp": 0.1, "analysis": 2},
      "low": {"main_loop": 0.3, "goal_gen": 0.2, "goal_comp": 0.15, "analysis": 3},
      "medium": {"main_loop": 0.6, "goal_gen": 0.4, "goal_comp": 0.3, "analysis": 5},
      "high": {"main_loop": 0.8, "goal_gen": 0.6, "goal_comp": 0.5, "analysis": 8},
      "maximum": {"main_loop": 0.9, "goal_gen": 0.8, "goal_comp": 0.7, "analysis": 12}
    },
    "intensity_levels": {
      "gentle": {"delay_multiplier": 2.0, "max_concurrent": 10, "actions_per_min": 5},
      "medium": {"delay_multiplier": 1.0, "max_concurrent": 25, "actions_per_min": 15},
      "aggressive": {"delay_multiplier": 0.5, "max_concurrent": 50, "actions_per_min": 30},
      "maximum": {"delay_multiplier": 0.2, "max_concurrent": 100, "actions_per_min": 60}
    }
  },
